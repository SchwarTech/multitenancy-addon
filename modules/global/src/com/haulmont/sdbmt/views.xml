<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<views xmlns="http://schemas.haulmont.com/cuba/view.xsd">
    <view class="com.haulmont.sdbmt.entity.Tenant"
          extends="_local"
          name="tenant-browse">
        <property name="accessGroup"
                  view="_minimal"/>
    </view>
    <view class="com.haulmont.sdbmt.entity.Tenant"
          extends="_local"
          name="tenant-edit">
        <property name="accessGroup"
                  view="_minimal">
            <property name="tenant" view="_minimal"/>
            <property name="parent" view="_minimal"/>
        </property>
    </view>
    <view class="com.haulmont.sdbmt.entity.MtGroup"
          extends="_local"
          name="group-tenant-and-hierarchy">
        <property name="tenant" view="_minimal"/>
        <property name="parent" view="_minimal"/>
        <property name="hierarchyList" view="_minimal">
            <property name="group" view="_minimal"/>
            <property name="parent" view="group-with-tenant"/>
        </property>
    </view>
    <view class="com.haulmont.sdbmt.entity.MtGroup"
          extends="_minimal"
          name="group-with-tenant">
        <property name="tenant" view="_minimal"/>
    </view>
    <view class="com.haulmont.sdbmt.entity.MtGroup"
          extends="_minimal"
          name="group-with-tenantId">
        <property name="tenantId"/>
    </view>
</views>
