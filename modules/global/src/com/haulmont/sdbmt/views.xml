<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<views xmlns="http://schemas.haulmont.com/cuba/view.xsd">
    <view class="com.haulmont.sdbmt.entity.Tenant"
          extends="_local"
          name="tenant-browse">
        <property name="group"
                  view="_minimal"/>
        <property name="admin"
                  view="_minimal"/>
    </view>
    <view class="com.haulmont.sdbmt.entity.Tenant"
          extends="_local"
          name="tenant-edit">
        <property name="group"
                  view="_minimal">
            <property name="tenant"
                      view="_minimal"/>
            <property name="parent"
                      view="_minimal"/>
        </property>
        <property name="admin"
                  view="_minimal"/>
    </view>
    <view class="com.haulmont.sdbmt.entity.SdbmtGroup"
          extends="_local"
          name="group-tenant-and-hierarchy">
        <property name="tenant"
                  view="_minimal"/>
        <property name="parent"
                  view="_minimal"/>
        <property name="hierarchyList"
                  view="_minimal">
            <property name="group"
                      view="_minimal"/>
            <property name="parent"
                      view="group-with-tenant"/>
        </property>
    </view>
    <view class="com.haulmont.sdbmt.entity.SdbmtGroup"
          extends="_minimal"
          name="group-with-tenant">
        <property name="tenant"
                  view="_minimal"/>
    </view>
    <view class="com.haulmont.sdbmt.entity.SdbmtGroup"
          extends="_minimal"
          name="group-with-tenantId">
        <property name="tenantId"/>
    </view>
    <view class="com.haulmont.sdbmt.entity.Order"
          extends="_local"
          name="order-browser-view">
        <property name="items"
                  view="_minimal">
            <property name="menuItem"
                      view="_minimal">
                <property name="name"/>
                <property name="price"/>
                <property name="hot"/>
            </property>
            <property name="quantity"/>
            <property name="comments"/>
        </property>
    </view>
    <view class="com.haulmont.sdbmt.entity.Order"
          extends="_local"
          name="order-edit-view">
        <property name="items"
                  view="_minimal">
            <property name="menuItem"
                      view="_minimal">
                <property name="name"/>
                <property name="price"/>
                <property name="hot"/>
            </property>
            <property name="quantity"/>
            <property name="comments"/>
        </property>
    </view>
    <view class="com.haulmont.sdbmt.entity.OrderItem"
          extends="_local"
          name="orderItem-edit-view">
        <property name="menuItem"
                  view="_minimal"/>
    </view>
    <view class="com.haulmont.sdbmt.entity.OrderItem"
          extends="_local"
          name="orderItem-browser-view">
        <property name="menuItem"
                  view="_minimal"/>
    </view>
    <view class="com.haulmont.sdbmt.entity.MenuItem"
          extends="_local"
          name="menuItem-browse">
        <property name="photo"
                  view="_local">
        </property>
    </view>
    <view class="com.haulmont.sdbmt.entity.MenuItem"
          extends="_local"
          name="menuItem-edit">
        <property name="photo"
                  view="_minimal"/>
    </view>
</views>
