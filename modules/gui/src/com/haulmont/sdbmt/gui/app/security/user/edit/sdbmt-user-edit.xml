<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        class="com.haulmont.sdbmt.gui.app.security.user.edit.SdbmtUserEditor"
        extends="/com/haulmont/cuba/gui/app/security/user/edit/user-edit.xml"
        messagesPack="com.haulmont.sdbmt.gui.app.security.user.edit">
    <dsContext>
        <datasource id="userDs" class="com.haulmont.sdbmt.entity.SdbmtUser"/>
    </dsContext>
    <layout>
        <groupBox id="propertiesBox">
            <grid id="propertiesGrid">
                <rows>
                    <row id="propertiesRow">
                        <fieldGroup id="fieldGroupRight">
                            <column>
                                <field id="tenantId"/>
                            </column>
                        </fieldGroup>
                    </row>
                </rows>
            </grid>
        </groupBox>
    </layout>
</window>
